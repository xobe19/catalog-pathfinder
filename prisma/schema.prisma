generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pair {
  address       String @id
  token0Address String
  token1Address String
  token0Reserve String
  token1Reserve String
}
