{
  "name": "catalog-pathfinder",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon server.ts",
    "build": "tsc && cp -r package* data .gitignore compose.yaml prisma dist/ && npx prisma generate",
    "deploy": "NODE_DEBUG=gh-pages node deploy.js",
    "start": "node server.js",
    "populate-db": "node src/data-fetcher/fetchPairDetailsAndStoreToDb.ts && node src/data-fetcher/fetchPairsAndUpdateDbSushi.js && node src/data-fetcher/fetchPairsAndUpdateDbPancake.js",
    "populate-db-uniswap": "node src/data-fetcher/fetchPairDetailsAndStoreToDb.js",
    "populate-db-sushiSwap": "node src/data-fetcher/fetchPairsAndUpdateDbSushi.js",
    "populate-db-panCakeSwap": "node src/data-fetcher/fetchPairsAndUpdateDbPancake.js",
    "populate-db-v3": "node src/data-fetcher/getAllPairsV3.js",
    "update-reserves": "node src/data-fetcher/updatePairDetailsInDb.js",
    "update-reserves-V3": "node src/data-fetcher/updatePairDetailsInDbV3.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@prisma/client": "^5.10.2",
    "@uniswap/v3-sdk": "^3.11.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "express": "^4.18.3",
    "graphql": "^16.8.1",
    "jsbi": "3.2.5",
    "mathjs": "^12.4.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.21",
    "@types/swagger-ui-express": "^4.1.6",
    "gh-pages": "^6.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.4.2"
  }
}